{
  "version": 2,
  "functions": {
    "backend_api.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend_api.py"
    },
    {
      "src": "/",
      "dest": "/mobile.html"
    },
    {
      "src": "/(.*)",
      "dest": "/mobile.html"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}